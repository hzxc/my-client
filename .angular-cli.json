{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "my-client"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "../node_modules/famfamfam-flags/dist/sprite/famfamfam-flags.min.css",
        "../node_modules/toastr/build/toastr.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/js-cookie/src/js.cookie.js",
        "../node_modules/lodash/lodash.min.js",
        "../node_modules/moment/min/moment.min.js",
        "../node_modules/signalr/jquery.signalR.js",
        "../node_modules/toastr/toastr.js",
        "../node_modules/block-ui/jquery.blockUI.js",
        "../node_modules/spin.js/spin.js",

        "./src/app/abp/resources/scripts/abp.js",
        "./src/app/abp/resources/scripts/libs/abp.jquery.js",
        "./src/app/abp/resources/scripts/libs/abp.toastr.js",
        "./src/app/abp/resources/scripts/libs/abp.blockUI.js",
        "./src/app/abp/resources/scripts/libs/abp.spin.js",
        "./src/app/abp/resources/scripts/libs/abp.moment.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
